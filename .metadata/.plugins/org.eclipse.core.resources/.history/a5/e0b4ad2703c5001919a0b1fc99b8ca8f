package lists;

import java.util.*;

public class Lists {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List cars = new ArrayList();
		
		cars.add("BMW");
		cars.add("SEAT");
		cars.add("Toyota");
		
		System.out.println(cars);
		System.out.println(cars.get(1));
		
		for(int i = 0; i < cars.size() ; i++)
		{
			System.out.println(cars.get(i));
		}
	}

}
